{
   "name": "my-tour",
   "version": "1.0.0",
   "scripts": {
      "start": "nodemon app.js"
   },
   "keywords": [],
   "author": "Andrii Myronenko",
   "engines": {
      "node": ">=18.19.1"
   },
   "dependencies": {
      "argon2": "^0.40.1",
      "connect-mongo": "^5.1.0",
      "dotenv": "^16.4.5",
      "express": "^4.18.3",
      "express-session": "^1.18.0",
      "express-validator": "^7.0.1",
      "mongoose": "^8.2.2",
      "passport": "^0.7.0",
      "passport-google-oauth20": "^2.0.0",
      "passport-local": "^1.0.0",
      "pino": "^8.19.0",
      "pino-http": "^9.0.0",
      "pino-pretty": "^10.3.1",
      "pug": "^3.0.2",
      "slugify": "^1.6.6",
      "undici": "^6.9.0",
      "validator": "^13.11.0"
   },
   "devDependencies": {
      "eslint": "^8.57.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-prettier": "^5.1.3",
      "nodemon": "^3.1.0",
      "prettier": "^3.2.5"
   },
   "eslintConfig": {
      "root": true,
      "extends": [
         "eslint:recommended",
         "plugin:node/recommended",
         "plugin:prettier/recommended"
      ],
      "plugins": [
         "prettier"
      ],
      "parserOptions": {
         "ecmaVersion": 2020
      },
      "rules": {
         "no-unused-vars": "warn"
      }
   },
   "prettier": {
      "printWidth": 120,
      "tabWidth": 3,
      "useTabs": false,
      "semi": true,
      "singleQuote": true,
      "quoteProps": "as-needed",
      "jsxSingleQuote": false,
      "trailingComma": "all",
      "bracketSpacing": true,
      "jsxBracketSameLine": true,
      "arrowParens": "always"
   }
}
